@-moz-document domain("youtube.com") {
    /* buttons rework */
    ytd-topbar-menu-button-renderer:nth-last-of-type(2) yt-icon {
        background: #f8f8f8;
        color: #171717;
        height: 28px !important;
        border: solid 1px #d3d3d3;
        padding: 0 10px !important;
        outline: 0;
        font: 500 11px roboto;
        border-radius: 2px;
        box-shadow: 0 1px 0 rgb(0 0 0 / 5%);
        vertical-align: middle;
        text-align: center;
        fill: none;
        filter: none;
        position: relative;
        top: -2px;
        width: auto;
    }
    ytd-topbar-menu-button-renderer:nth-last-of-type(2) yt-icon:before {
        content: "Upload";
        display: inline-block;
        color: #171717;
        letter-spacing: 0;
        font: 500 12px Roboto, sans-serif!important;
    }

    ytd-topbar-menu-button-renderer:nth-last-of-type(2) yt-icon:hover {
        border-color: #c6c6c6;
        background: #f0f0f0;
    }

    ytd-button-renderer.style-suggestive[is-paper-button] tp-yt-paper-button.ytd-button-renderer,
    html:not([dark]) ytd-button-renderer.style-suggestive[is-paper-button] tp-yt-paper-button.ytd-button-renderer {
        border-color: #167ac6!important;
        background: #167ac6!important;
        color: #fff;
        border-radius: 2px!important;
        box-shadow: 0 1px 0 rgb(0 0 0 / 5%);
        font: bolder 11px roboto!important;
        height: 28px!important;
        padding: 0 10px;
    }
    /* moving the newly (not) remade buttons */
    .yt-spec-icon-badge-shape__icon {
        margin-right: -100px;
    }
    ytd-topbar-menu-button-renderer:nth-last-of-type(2) yt-icon {
        margin-left: -15px;
    }

    ytd-button-renderer.style-suggestive[is-paper-button] tp-yt-paper-button.ytd-button-renderer,
    html:not([dark]) ytd-button-renderer.style-suggestive[is-paper-button] tp-yt-paper-button.ytd-button-renderer {
        margin-left: 15px;
    }
    /* 2015 stuff?*/
    yt-img-shadow.ytd-topbar-menu-button-renderer {
        border-radius: 0px;
    }

    ytd-mini-guide-renderer.style-scope.ytd-app {
        display: none;
    }
    /* dont know if this works properly*/
    #guide-inner-content.ytd-app {
        border-right: 1px solid #e8e8e8!important;
        box-shadow: none!important;
    }
    /* the literal 3rd searchbar rework today */
    #masthead-container #search-icon-legacy.ytd-searchbox yt-icon.ytd-searchbox {
        background: no-repeat url(//web.archive.org/web/20151030043928im_/https://s.ytimg.com/yts/imgbin/www-hitchhiker-vflUTAH46.png) -108px -1003px;
        background-size: auto;
        width: 14px;
        height: 14px;
        box-shadow: none;
    }

    #masthead #search-form.ytd-searchbox {
        background-color: #fff;
        font-size: 14px;
        height: 29px;
        line-height: 30px;
        margin: 0 0 2px;
        overflow: hidden;
        position: relative;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
        -moz-transition: border-color .2s ease;
        -webkit-transition: border-color .2s ease;
        transition: border-color .2s ease;
    }

    #masthead #search-icon-legacy.ytd-searchbox {
        border-color: #ccc;
        background: #f8f8f8;
    }

    #search-clear-button.ytd-searchbox {
        display: none;
    }
    /* appbar */
    .ytcp-main-appbar {
        background-color: #fff;
        border-bottom: 1px solid #e8e8e8;
        height: 40px;
        line-height: 40px;
    }


    #items > ytd-guide-entry-renderer:nth-child(5) {
        display: none !important;
    }
}

@-moz-document url-prefix("https://www.youtube.com/watch") {
span.ytd-video-view-count-renderer,
ytd-video-primary-info-renderer[has-date-text] #info-text.ytd-video-primary-info-renderer {
    top: -30px!important;
}
ytd-app #info .ryd-tooltip,
#info #sentiment.ytd-video-primary-info-renderer {
    top: -1px!important;
}
#info-contents[hidden] #upload-info > #owner-sub-count {
    margin-right: 181px!important;
    margin-left: 152px!important;
    /* this is used to cancel out the other code */
}
/* code from guy who we should bow down to*/
#info.ytd-video-primary-info-renderer {
    height: 20px !important;
}

#info ytd-video-owner-renderer {
    top: -21px !important;
}

ytd-app #channel-name.ytd-video-owner-renderer {
    right: 0;
    top: -16px !important;
}

ytd-app #subscribe-button.ytd-video-secondary-info-renderer {
    top: 17px;
}

#menu.ytd-video-primary-info-renderer {
    top: 7px !important;
}


#info ytd-video-primary-info-renderer[has-date-text] #info-text.ytd-video-primary-info-renderer {
    top: -32px;
}

#meta ytd-video-secondary-info-renderer {
    padding: 1px 15px;
}

/* old subs look */
#owner-sub-count.ytd-video-owner-renderer {
    border: 1px solid #ccc;
    background-color: #fafafa;
    vertical-align: middle;
    border-radius: 0 2px 2px 0!important;
    padding: 0 6px!important;
    color: #737373;
    font-size: 11px!important;
    text-align: center;
}

html #subscribe-button ytd-button-renderer #button.ytd-button-renderer,
html #subscribe-button tp-yt-paper-button.ytd-subscribe-button-renderer {
    padding: 0 8px 0 5.5px;
    height: 24px;
    color: #fefefe;
    background-color: #e62117;
}

.style-scope.ytd-menu-renderer.force-icon-button.style-default.size-default:nth-of-type(1) yt-icon {
    background: no-repeat url(https://s.ytimg.com/yts/imgbin/www-hitchhiker-vflY-jC_p.png) -32px -2211px!important;
    color: #fff;
    fill: none!important;
}
/* fixing aubys auto play (very real) *
ytd-compact-autoplay-renderer {
    padding: 12px 0 15px 15px !important;
         box-shadow: 0 1px 2px rgb(0 0 0 / 10%) !important;
    background-color: #fff !important;
    width: 402px;
}*/
.ytp-chrome-bottom {
    height: 36px!important;
}

.ytp-menuitem:not([aria-disabled=true]) {
    border-bottom: 1px #bbb;
}




#more-replies yt-formatted-string,
#less-replies yt-formatted-string {
    text-transform: lowercase;
}

#more-replies yt-formatted-string::before {
    content: "View ";
    text-transform: none;
}

#less-replies yt-formatted-string::before {
    content: "Hide ";
    text-transform: none;
}
.less-button.ytd-comment-renderer,
tp-yt-paper-button.ytd-expander .more-button.ytd-comment-renderer,
ytd-comment-replies-renderer #button.ytd-button-renderer yt-icon.ytd-button-renderer + yt-formatted-string.ytd-button-renderer {
    text-transform: lowercase !important;
}
}

@-moz-document url("https://www.youtube.com/") {
/* homepage */
html:not([dark]) ytd-app yt-formatted-string[has-link-only_]:not([force-default-style]) a.yt-simple-endpoint.yt-formatted-string:before {
    Content: 'by ';
    color: #767676;
}
html:not([dark]) ytd-app yt-formatted-string[has-link-only_]:not([force-default-style]) a.yt-simple-endpoint.yt-formatted-string:hover {
    color: #167ac6!important;
}
}
